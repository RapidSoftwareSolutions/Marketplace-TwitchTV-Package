    {
      "name": "getChannelVideos",
      "description": "Get channel's list of videos",
      "args": [
        {
          "name": "clientId",
          "type": "credentials",
          "info": "Application client id",
          "required": true
        },
        {
          "name": "channel",
          "type": "String",
          "info": "Channel to video retrieve from",
          "required": true
        },
        {
          "name": "limit",
          "type": "Number",
          "info": "Maximum number of objects in array. Default is 10. Maximum is 100",
          "required": false
        },
        {
          "name": "offset",
          "type": "Number",
          "info": "Object offset for pagination. Default is 0",
          "required": false
        },
        {
          "name": "broadcasts",
          "type": "Select",
          "options":["true","false"],
          "info": "Returns only broadcasts when `true`. Otherwise only highlights are returned. Default is `false`",
          "required": false
        },
        {
          "name": "hls",
          "type": "Select",
          "options":["true","false"],
          "info": "Returns only HLS VoDs when `true`. Otherwise only non-HLS VoDs are returned. Default is `false`",
          "required": false
        },
        {
          "name": "broadcastType",
          "type": "list",
          "structure":{
            "name":"type",
            "type":"Select",
            "options":["archive","highlight","upload"],
            "info":"Single type."
          },
          "info": "Constrains the type of videos returned. Valid values: (any combination of) archive, highlight, upload. Default: all types (no filter).",
          "required": false
        },
        {
          "name": "language",
          "type": "List",
          "structure":{
            "name":"language",
            "type":"String",
            "info":"Single language."
          },
          "info": "Constrains the language of the videos that are returned; for example, en,es. Default: all languages.",
          "required": false
        },
        {
          "name": "sort",
          "type": "Select",
          "options":["views","time"],
          "info": "Sorting order of the returned objects. Valid values: views, time. Default: time (most recent first).",
          "required": false
        }
      ]
    },
