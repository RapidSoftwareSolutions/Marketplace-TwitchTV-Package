      "name": "getAccessToken",
      "description": "Getting access tokens",
      "args": [
        {
          "name": "clientId",
          "type": "credentials",
          "info": "Application client id",
          "required": true
        },
        {
          "name": "clientSecret",
          "type": "credentials",
          "info": "Application client secret key",
          "required": true
        },
        {
          "name": "code",
          "type": "String",
          "info": "The authorization code",
          "required": true
        },
        {
          "name": "redirectUri",
          "type": "String",
          "info": "Application redirect url",
          "required": true
        },
        {
          "name": "state",
          "type": "String",
          "info": "Your provided unique token",
          "required": false
        }
      ],
      "callbacks": [
        {
          "name": "error",
          "info": "Error"
        },
        {
          "name": "success",
          "info": "Success"
        }
      ]
    },

